	doi = {10.1093/scan/nsq070},
	eprint = {https://academic.oup.com/scan/article-pdf/6/4/495/27106462/nsq070.pdf},
	issn = {1749-5016},
	journal = {Social Cognitive and Affective Neuroscience},
	keywords = {fMRI, classical conditioning, evaluative conditioning, skin conductance response},
	month = {08},
	number = {4},
	pages = {495--506},
	title = {Influence of contingency awareness on neural, electrodermal and evaluative responses during fear conditioning},
	url = {https://doi.org/10.1093/scan/nsq070},
