@article{SCHULTZ2010,
	abstract = {The role of contingency awareness in classical conditioning experiments using human subjects is currently under debate. This study took a novel approach to manipulating contingency awareness in a differential Pavlovian conditioning paradigm. Complex sine wave gratings were used as visual conditional stimuli (CS). By manipulating the fundamental spatial frequency of the displays, we were able to construct pairs of stimuli that varied in discriminability. One group of subjects was given an ```easy''` discrimination, and another was exposed to a ```difficult''` CS and CS--. A 3rd group was exposed to a stimulus that was paired with the unconditional stimulus (UCS) 50\% of the time and served as a control. Skin conductance response (SCR) and continuous UCS expectancy data were measured concurrently throughout the experiment. Differential UCS expectancy was found only in the easy discrimination group. Differential SCRs were found in the easy discrimination group as well as in the difficult discrimination group, but not in the 50\% contingency control. The difficult discrimination group did not exhibit differential UCS expectancy but did show clear differential SCR. These observations support a dual process interpretation of classical conditioning whereby conditioning on an implicit level can occur without explicit knowledge about the contingencies.},
	annote = {- Kontingenzawareness und SCR mit zwei verschiedenen CS Sets: einfach und schwere Sinus-Wellen
- Kontrollgruppe: 50\% UCS Reinforcement
- Support einer dual-process interpretation: differentielle UCS expectancy nur in einfacher Gruppe, aber in beidne Gruppen differentielle SCR


- data from human participants with brain lesions (Bechara et al., 1995) have supported a dual process theory of conditioning that states that implicit and explicit processes are dissociable and that conditioning can occur independently of explicit contingency awareness ---> siehe Gazzaniga, doppelte Dissoziation
- {\"U}berlegung, dass unterschiedliche Hirnareale bei impliziten und explizitem Lernen eine Rolle spielen

- Kritik von Lovibond \& Shanks: aufgearbeitet von z.B. Smith, Clark, Manns \& Squire, 2005; Knight, Ngyen und Bandettini,)
- Findings suggest: delay conditioning is independent of contingency awareness

- UCS: painful but tolerable ---> also eher pain conditioning oder?
- Two participants in the difficult discrimination group demonstrated contingency awareness and were excluded from further analysis
- (Tabbert, Stark, Kirsch,  Vaitl, 2006) identified differential brain activity between participants who were either aware or unaware of the stimulus contingencies in a fear conditioning paradigm

- The current study developed a set of visual stimuli that can serve as CSs in a conditioning experiment and affect the degree of contingency awareness without having to use a distracter task concurrently

- pseudorandom trial sequence with the restriction that there were no more than two consecutive trials of the same type. Various human conditioning studies have used these same parameters (Cheng, Knight, Smith, and Helmstetter, 2006; LaBar, Cook, Torpey, and Welsh-Bohmer, 2004). Given these circumstances, participants could potentially be learning something about the trial sequence because it is not truly random ---> learning about the trial sequence
- durch Kontrollgruppe hier: Trial Learning ist nicht verantwortlich f{\"u}r die Effekte





},
	author = {Douglas H. Schultz and Fred J. Helmstetter},
	bdsk-color = {35802111},
	date-added = {2020-10-17 15:14:58 +0200},
	date-modified = {2020-12-22 15:55:09 +0100},
	doi = {10.1037/a0020263},
	journal = {Journal of Experimental Psychology: Animal Behavior Processes},
	number = {4},
	pages = {495--500},
	read = {1},
	title = {Classical conditioning of autonomic fear responses is independent of contingency awareness},
	url = {https://doi.org/10.1037%2Fa0020263},
	volume = {36},
	year = 2010,
	Bdsk-Url-1 = {https://doi.org/10.1037%2Fa0020263},
	Bdsk-Url-2 = {https://doi.org/10.1037/a0020263}}

@article{SEVENSTER2014,
	abstract = {There is conflicting evidence as to whether awareness is required for conditioning of the skin conductance response (SCR). Recently, Schultz and Helmstetter reported SCR conditioning in contingency unaware participants by using difficult to discriminate stimuli. These findings are in stark contrast with other observations in human fear conditioning research, showing that SCR predominantly reflects contingency learning. Therefore, we repeated the study by Schultz and Helmstetter and additionally measured conditioning of the startle response, which seems to be less sensitive to declarative knowledge than SCR. While we solely observed SCR conditioning in participants who reported awareness of the contingencies (n = 16) and not in the unaware participants (n = 18), we observed startle conditioning irrespective of awareness. We conclude that SCR but not startle conditioning depends on conscious discriminative fear learning.},
	annote = {- Reaktion auf Schultz,2010: deren Findings sind im starken kontrast zu sonstigen, wo SCR {\"u}berm{\"a}{\ss}ig das Kontingenzlernen reflektiert
- daher: Wiederholung des Experiments, aber zus{\"a}tzlich noch Startle, der weniger sensitiv gegen{\"u}ber deklaratibem Wissen als SCR sein soll

- SCR auch ohne Kontingenzawareness (Knight et al, 2003, Bechara, 1995) oder SCR ausschlie{\ss}lich mit US-Expectancy (Hamm, 1996, Purkis und Lipp, 2001, Lovibond 2002, Hamm und Weike 2005, Weike 2007)
- aber: unter Bedingungen die Awareness und Konditionierung von SCR verhindern, war Startle Potenzierung m{\"o}glich (Hamm, 1996, Weike 2007)
- auch pharmakologische und kogntivie Manipulationen lassen vermuten, dass SCR stark mit expectancy learning zusammenh{\"a}ngt, z.B. Propranolol (Soeter und Kindt, 2010; Sevenster 2012)
- results suggest that SCR responding appears to mirror expectancy beliefs (see also Lovibond, 2004), whereas the startle response can act independent from cognitive knowledge.
- Schultz, 2010 findings sind erstaunlich gg{\"u}. der starken Konvergenz zwischen Expectancy Learning und SCR 

- h{\"o}here Sample Size um differentielle SCR in VP die unintentional Awareness aufgebaut haben und Startle 
- Verweis auf Singh, 2013:  Trial sequenz als Grund f{\"u}r unaware SCR conditioning ---> denn predictable Trial Sequence konnte unaware conditioning effects erkl{\"a}ren (Wiens 2003)
- daher auch predictable Stimulus presentation mit explored

- hier unbedingt mal gucken, wie Hautleit und Startle erfassung dokumentiert werden k{\"o}nnen

- Expectancy: Unterschiedlich f{\"u}r leicht aber nicht f{\"u}r schwere Bedingung.
- SCR: unterschiedlich f{\"u}r leicht aber nicht f{\"u}r schwere Bedingung > Thus in contrast to Schultz and Helmstetter (2010), we found that contingency awareness is a prerequisite for SCR conditioning
- Startle: einfach und shcwer differneziertern weder bei startle habituation noch bei Startle w{\"a}hrend ITI > aber bei beiden ist CS+ h{\"o}her als CS-
- Trial Sequenz: CR wurden gemittelt einmal f{\"u}r CS trials denen ein gleicher CS-Typ vs. ein anderer CS-Typ vorherging: predictable trial sequenz hat das expectancy lernen vereinfacht, (bei SCR nur in der einfachen Version, bei Startle gar nicht) 

- transient physiological conditioning effect contrasts with our previous studies in which we did observe SCR and startle conditioning during late acquisition (Soeter and Kindt, 2010, Sevenster 2012)
- keine Replikation der Schultz Ergebnisse
- predictable trial sequence may have resultated in unintended contingecy awareness
- stresses the urge to control for trial sequence in fear conditioning research, especially when using SCR as a measure of conditioned responding.
- the current findings suggest that fear learning involves at least two processes


},
	author = {Sevenster, Dieuwke and Beckers, Tom and Kindt, Merel},
	bdsk-color = {35802111},
	date-added = {2020-10-17 15:11:38 +0200},
	date-modified = {2020-12-18 13:44:31 +0100},
	doi = {10.3389/fnbeh.2014.00032},
	issn = {1662-5153},
	journal = {Frontiers in Behavioral Neuroscience},
	keywords = {fear conditioning, awareness, skin conductance response, fear potentiated startle, contingency learning},
	month = {02},
	number = {32},
	read = {1},
	title = {Fear conditioning of SCR but not the startle reflex requires conscious discrimination of threat and safety},
	url = {https://www.frontiersin.org/article/10.3389/fnbeh.2014.00032},
	volume = {8},
	year = {2014},
